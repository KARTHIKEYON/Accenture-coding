import java.util.*;
class Main{
    public static void main(String[] args){
     int arr[]={4,5,6,7,8,3};
     int evecount=0;
     evecount+=arr[0];
     int oddcount=0;
     for(int i=1;i<arr.length;i++){
         if(i%2==0){
             evecount+=arr[i];
         }
         else{
             oddcount+=arr[i];
         }
     }
     int tuple[]={oddcount,evecount};
     System.out.println(tuple[0]+" "+tuple[1]);
    }
}


output:
15 18
